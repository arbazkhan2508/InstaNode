<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.3/jquery.min.js"></script>
<script>

  var count = 0;
    document
      .querySelector(".menu")
      .addEventListener("click", function () {
        if (count == 0) {
          document.querySelector("#morediv").style.top = "43%";
          document.querySelector("#morediv").style.display = "initial";
          count = 1;
        } else {
          document.querySelector("#morediv").style.top = "100%";
          document.querySelector("#morediv").style.display = "none";
          count = 0;
        }
      });
    // ritik_js

    var count = 0;
    document
      .querySelector("#search_user")
      .addEventListener("click", function () {
        if (count == 0) {
          document.querySelector("#search-div").style.left = "15%";
          count = 1;
        } else {
          document.querySelector("#search-div").style.left = "-42%";
          count = 0;
        }
      });

    $("#searchform").submit(function (event) {
      event.preventDefault();
      var search = $("#search").val();
      console.log(search
      );

      $.ajax({
        url: "/searchUser",
        type: "POST",
        data: {
          search: search,
        },
        success: function (response) {
          if (response.success) {
            $("#searchResult").html("");
            $("#search").val("");
            let user = response.data;
            user.forEach(function (user) {
              console.log(user.username);

              var a = document.createElement("a");
              a.href = `user/${user.name}`;
              var div = document.createElement("div");
              div.classList.add("user");
              var Img = document.createElement("img");
              Img.src = `/getFile/${user.userProfileimg}`;
              var h4 = document.createElement("h4");
              h4.textContent = user.username;
              div.appendChild(Img);
              div.appendChild(h4);
              a.appendChild(div);
              $("#searchResult").append(a);
            });
          }else{
              console.log("heyyyyyxxxxxxx");
              alert("heyy ")
          }
        },
      });
    });

    // ritik_js
</script>